{
  "name": "slidev-theme-jetslide",
  "version": "0.0.0",
  "type": "module",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "slidev build slides.md",
    "dev": "slidev slides.md --open",
    "export": "slidev export slides.md",
    "screenshot": "slidev export slides.md --format png"
  },
  "dependencies": {
    "@slidev/theme-default": "^0.25.0",
    "@slidev/types": "^0.50.0-beta.4"
  },
  "devDependencies": {
    "@slidev/cli": "^0.50.0-beta.4",
    "playwright-chromium": "^1.56.0"
  },
  "//": "Learn more: https://sli.dev/guide/write-theme.html",
  "slidev": {
    "colorSchema": "both",
    "defaults": {
      "fonts": {
        "sans": "Nunito Sans",
        "mono": "Fira Code"
      }
    }
  }
}